<%-contentFor('providedcss')%>
<link href="/stylesheets/new.css" rel="stylesheet">
<%-contentFor('body')%>
<div class="container">
<div class="col-sm-offset-3 col-sm-6">
<form class="form-horizontal" role="form" method="post" action="/rooms/create">
  <div class="form-group">
    <label for="inputroom" class="col-sm-4 control-label">Enter room name</label>
    <div class="col-sm-8">
      <input type="text" class="form-control" id="inputroom" name="name" placeholder="Enter room name">
    </div>
  </div>
  <div class="form-group">
    <label for="inputdescription" class="col-sm-4 control-label">Enter room description</label>
    <div class="col-sm-8">
      <input type="text" class="form-control" id="inputdescription" name="description" placeholder="Enter room description">
    </div>
  </div>
  <div class="form-group">
    <label for="inputuser" class="col-sm-4 control-label">Enter user name</label>
    <div class="col-sm-8">
      <input type="text" class="form-control" id="inputuser" name="user" placeholder="Enter user name">
    </div>
  </div>
  </br>
  <div class="form-group">
    <div class="col-sm-offset-4 col-sm-6">
      <button type="submit" class="btn btn-lg btn-primary">Create</button>
    </div>
  </div>
</form>
</div>
</div>